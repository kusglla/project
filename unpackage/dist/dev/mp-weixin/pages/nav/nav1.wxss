/* 购物分类 */
.buytype-wrap{
	width: 709rpx;
	margin: 16rpx auto 0rpx;
	padding: 30rpx 0rpx;
	border-radius: 10rpx;
	background:rgba(255,255,255,1);
	box-shadow:0px 4px 9px 0px rgba(0, 0, 0, 0.07);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.buytype-wrap image{
	width: 57rpx;
	height: 56rpx;
	margin-top: 10rpx
}
.buy-item{
	width: 125rpx;
	text-align: center;
	margin-top: 30rpx;
}
.img-bg3{
	height: 72rpx;
	width: 72rpx;
	border-radius: 50%;
	background: #FF7F3B;
	text-align: center;
}
.img-bg3>image{
	height: 39rpx;
	width: 32rpx;
	margin-top: 20rpx;
}
.img-bg7{
	width: 66rpx;
	height: 79rpx;
	text-align: center;
	background: url(data:image/png;base64,)no-repeat;
	background-size: 100% 100%;
}
.img-bg7>image{
	height: 38rpx;
	width: 35rpx;
	margin-top: 15rpx;
}

/* 搜索栏 */
.search-box{
	width:750rpx;
	height:101rpx;
	background:rgba(225,180,95,1);
}

/* order */
.order-list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 30rpx 40rpx;
	box-sizing: border-box;
}
.order-item{
	width: 308rpx;
	margin-top: 40rpx;
}
.order-item>view:nth-child(1){
	width: 308rpx;
	height: 223rpx;
	border-radius: 10rpx;
}
.order-item>view:nth-child(2){
	width: 305rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size:28rpx;
	font-weight:500;
	color:rgba(51,51,51,1);
	margin-top: 23rpx;
}
.order-item>view:nth-child(3){
	font-size:32rpx;
	font-weight:500;
	color:rgba(251,70,50,1);
	margin-top: 23rpx;
}
.order-item>view:nth-child(4){
	font-size:24rpx;
	font-weight:500;
	color:rgba(153,153,153,1);
	margin-top: 7rpx;
}

