
.share-item{
	margin-top: 20rpx;
	padding: 0rpx 35rpx;
	border-bottom: 1px solid #CCCCCC;
}
.item-top{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-bottom: 1rpx dashed #EEEEEE;
	padding-bottom: 26rpx;
}
.item-top>.left{
	width: 184rpx;
	height: 191rpx;
	border-radius: 10rpx;
}
.item-top>.right{
	margin-left: 23rpx;
}
.item-top>.right>view:nth-child(1){
	font-size: 30rpx;
	color: #333333;
}
.item-top>.right>view:nth-child(2){
	font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	color: #666666;
	margin-top: 19rpx;
}
.item-top>.right>view:nth-child(3){
	font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	letter-spacing: 2rpx;
	color: #666666;
	margin-top: 36rpx;
}
.item-top>.right>view:nth-child(4){
	margin-top: 15rpx;
}
.item-bottom{
	font-size: 26rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 25rpx 0rpx;
}
.item-bottom>view:nth-child(1){
	font-size: 26rpx;
	letter-spacing: 2px;
	color: #666666;
}
.item-bottom>view:nth-child(2){
	margin-left: 253rpx;
}
.item-bottom>view:nth-child(3){
	margin-left: 60rpx;
}
.warn{
	font-size: 24rpx;
	color: #666666;
	margin-top: 266rpx;
	margin-bottom: 40rpx;
	text-align: center;
}

