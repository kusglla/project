/* 导航栏 */
.nav-wrap{
	height: 190rpx;
	padding: 21rpx;
	width: 709rpx;
	margin: auto;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.nav-item{
	width:102rpx;
	height:102rpx;
	border-radius:50%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.nav-wrap>view:nth-child(1)>.nav-item{
	background:-webkit-linear-gradient(108deg,rgba(225,180,95,1),rgba(251,207,150,1));
	background:linear-gradient(-18deg,rgba(225,180,95,1),rgba(251,207,150,1));
}
.nav-wrap>view:nth-child(2)>.nav-item{
	background:-webkit-linear-gradient(135deg,rgba(252,107,87,1),rgba(251,56,37,1));
	background:linear-gradient(-45deg,rgba(252,107,87,1),rgba(251,56,37,1));
}
.nav-wrap>view:nth-child(3)>.nav-item{
	background:-webkit-linear-gradient(135deg,rgba(241,85,133,1),rgba(250,146,183,1));
	background:linear-gradient(-45deg,rgba(241,85,133,1),rgba(250,146,183,1));
}
.nav-wrap>view:nth-child(4)>.nav-item{
	background:-webkit-linear-gradient(135deg,rgba(112,168,254,1),rgba(72,127,254,1));
	background:linear-gradient(-45deg,rgba(112,168,254,1),rgba(72,127,254,1));
}
.nav-wrap>view:nth-child(5)>.nav-item{
	background:-webkit-linear-gradient(135deg,rgba(17,168,101,1),rgba(124,236,118,1));
	background:linear-gradient(-45deg,rgba(17,168,101,1),rgba(124,236,118,1));
}
.nav-item image{
	height: 46rpx;
	width: 46rpx;
}

