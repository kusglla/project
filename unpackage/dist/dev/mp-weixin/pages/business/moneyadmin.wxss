





































































/* 累计奖励 */
.moneysum-wrap{
	background: #FFFFFF;
	padding: 35rpx 35rpx 45rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.moneysum-wrap>view:nth-child(1){}
.moneysum-wrap>view:nth-child(2){
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
/* 查询账目日期 */
.daymoney-wrap{
	background-color: #FFFFFF;
	padding: 25rpx 40rpx 50rpx;
}
.daymoney-wrap>view:nth-child(1){}
.daymoney-wrap>view:nth-child(2){
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-top: 65rpx;
	font-size: 28rpx;
	color: #333333;
	text-align: center;
}
/* 线下消费 */
.cutmoney-wrap{
	box-sizing: border-box;
	background: #FFFFFF;
}
.cutmoney-item{
	border-top: 1px solid #EEEEEE;
	padding: 40rpx 40rpx 55rpx;
}
.cutmoney-item>view:nth-child(1){
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.cutmoney-item>view:nth-child(2){
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-top: 25rpx;
}
.cutmoney-item>view:nth-child(3){
	width: 100%;
	height: 115rpx;
	box-sizing: border-box;
	background: #F1F1F1;
	padding: 20rpx 25rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 35rpx;
}

