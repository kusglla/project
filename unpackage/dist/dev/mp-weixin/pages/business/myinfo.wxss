
.msg-wrap{
	margin: 0rpx 40rpx;
	box-sizing: border-box;
}
.msg-item{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 85rpx;
}
.msg-item>view{
	width: 200rpx;
}
.msg-item>view:nth-child(1){
	width: 200rpx;
}
.msg-item>view:nth-child(2){
	margin-left: 40rpx;
}
input{
	width: 420rpx;
}

